{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/docs",
      "destination": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  }
}
